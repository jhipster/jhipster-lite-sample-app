{
  "name": "litesample",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Lite Sample Application",
  "license": "UNLICENSED",
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --"
  },
  "dependencies": {
    "axios": "1.6.5",
    "vue": "3.4.15",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "3.2.2",
    "@rushstack/eslint-patch": "1.7.0",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitest/coverage-istanbul": "1.2.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.20.1",
    "husky": "8.0.3",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.5.0",
    "prettier-plugin-packagejson": "2.4.9",
    "sinon": "17.0.1",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vitest": "1.2.1",
    "vitest-sonar-reporter": "1.0.0",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
