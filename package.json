{
  "name": "litesample",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Lite Sample Application",
  "license": "UNLICENSED",
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prepare": "husky",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --"
  },
  "dependencies": {
    "axios": "1.6.7",
    "vue": "3.4.20",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "3.3.1",
    "@rushstack/eslint-patch": "1.7.2",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-istanbul": "1.3.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.4",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.22.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.6.0",
    "prettier-plugin-packagejson": "2.4.12",
    "sinon": "17.0.1",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vitest": "1.3.1",
    "vitest-sonar-reporter": "2.0.0",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
