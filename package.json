{
  "name": "litesample",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Lite Sample Application",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.17.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\""
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.2",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/test-utils": "2.4.1",
    "@vitest/coverage-istanbul": "0.34.1",
    "eslint": "8.46.0",
    "eslint-plugin-vue": "9.16.1",
    "jsdom": "22.1.0",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vitest": "0.34.1",
    "vitest-sonar-reporter": "0.4.1",
    "vue-tsc": "1.8.8",
    "@types/sinon": "10.0.16",
    "sinon": "15.2.0",
    "@prettier/plugin-xml": "3.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prettier-plugin-java": "2.2.0",
    "prettier-plugin-packagejson": "2.4.5"
  },
  "dependencies": {
    "vue": "3.3.4",
    "axios": "1.4.0",
    "vue-router": "4.2.4"
  }
}
