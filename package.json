{
  "name": "litesample",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Lite Sample Application",
  "license": "UNLICENSED",
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --"
  },
  "dependencies": {
    "axios": "1.3.4",
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.2.0",
    "@rushstack/eslint-patch": "1.2.0",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/parser": "5.57.0",
    "@vitejs/plugin-vue": "4.1.0",
    "@vitest/coverage-istanbul": "0.29.7",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.3.2",
    "eslint": "8.36.0",
    "eslint-plugin-vue": "9.10.0",
    "husky": "8.0.3",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "prettier-plugin-java": "2.1.0",
    "prettier-plugin-packagejson": "2.4.3",
    "sinon": "15.0.3",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7",
    "vitest-sonar-reporter": "0.4.0",
    "vue-tsc": "1.2.0"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
